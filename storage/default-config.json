        },
        {
            "name": "discord",
            "src": "./modules/discord/index.js",
            "enabled": false,
            "settings": {
                "dev": {
                    "deploy_commands": false
                },
                "client_id": "YOUR_CLIENT_ID",
                "token": "YOUR_BOT_TOKEN"
            }
        }
    ]
}